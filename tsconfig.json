{
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "strict": true,
    "moduleResolution": "Bundler",
    "skipLibCheck": true,
    "importHelpers": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
  }
}

